{
	"version": "2.0.0",
        "configurations": [

        {
            "name": "Cortex-Debug Zephyr VSCode",
            "cwd": "${workspaceRoot}/../",
            "executable": "zephyr/build/zephyr/zephyr.elf",
            "request": "launch",
            "type": "cortex-debug",
            "servertype": "openocd",
            "interface": "swd",
            "device": "stm32f303re",
            "targetId": "nucleo_f303re",
            "boardId": "1",
            "openocdPath": "/usr/local/bin",
            "armToolchainPath": "${env:HOME}/zephyr-sdk/arm-zephyr-eabi/bin",
            "gdbpath": "${env:HOME}/zephyr-sdk/arm-zephyr-eabi/bin/arm-zephyr-eabi-gdb",
            // "gdb_port": "6000",
            "svdFile": "${workspaceRoot}/stm32_svd/STM32F303xE.svd",
            "showDevDebugOutput": false,
            "configFiles": [
                        "zephyr/boards/arm/nucleo_f303re/support/openocd.cfg"
                ]
        }
    ]
}
